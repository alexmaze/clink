{
  "extends": "tslint:latest",
  "rulesDirectory": [
    "node_modules/tslint-microsoft-contrib"
  ],
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true,
      "double",
      "jsx-double",
      "avoid-escape"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "no-string-literal": false,
    "no-console": [
      true,
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "comment-format": [
      true,
      "check-space"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      },
      {
        "call-signature": "space",
        "index-signature": "space",
        "parameter": "space",
        "property-declaration": "space",
        "variable-declaration": "space"
      }
    ],
    "only-arrow-functions": [
      false
    ],
    "no-consecutive-blank-lines": true,
    "no-empty-interface": false,
    "variable-name": [
      true,
      "ban-keywords"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type"
    ],
    "align": [
      true,
      "parameters",
      "statements"
    ],
    "max-classes-per-file": [
      false
    ],
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "array-type": [
      true,
      "array"
    ],
    "max-line-length": [
      true,
      180
    ]
  }
}